<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.learnnavi.app"
    android:versionCode="170"
    android:versionName="0.8.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="26" >
    </uses-sdk>

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/ThemeSelector" >
        <activity
            android:name="org.learnnavi.app.Kelutral"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.learnnavi.app.Dictionary"
            android:exported="false"
            android:launchMode="singleTop" >
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name="org.learnnavi.app.Preferences"
            android:exported="false" >
        </activity>
        <!--
		<activity 
			android:name="HorenBrowse" 
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>		
        -->
        <provider
            android:name="org.learnnavi.app.NaviWords"
            android:authorities="org.learnnavi.mobile.naviword" >
        </provider>
        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="org.learnnavi.app.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>